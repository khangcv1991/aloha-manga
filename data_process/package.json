  "description": "",
  "devDependencies": {
    "serverless-dynamodb-local": "^0.2.40",
    "serverless-dynamodb-seed": "^0.3.0",
    "serverless-offline": "^12.0.4",
    "serverless-offline-watcher": "^1.1.0",
    "serverless-python-requirements": "^6.0.0"
  }
}
